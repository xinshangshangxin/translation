{
    "content_scripts": [{
        "all_frames": true,
        "js": ["googletranslater.js"],
        "matches": ["http://*/*", "https://*/*", "file://*/*"],
        "run_at": "document_end"
    }],
    "description": "谷歌划词翻译",
    "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "incognito": "spanning",
    "manifest_version": 2,
    "name": "使用谷歌翻译api,实现划词翻译",
    "permissions": ["*://*/*"],
    "version": "1.0.2"
}
